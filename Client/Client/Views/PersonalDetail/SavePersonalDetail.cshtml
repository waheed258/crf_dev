@{
    ViewBag.Title = "SavePersonalDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="@Url.Content("~/Scripts/angular.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ng/ngApp/ngApp.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ng/ngService/ngCrudService.js")" type="text/javascript"></script>
<script src="@Url.Content("~/ng/ngControllers/ngClientProfileController.js")" type="text/javascript"></script>




<div class="content-wrapper" ng-controller="ClientProfileController" ng-app="myapp">
    <form name="ClientProfileForm" ng-submit="Save()">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="header-icon">
                <i class="fa fa-users"></i>
            </div>
            <div class="header-title">
                <h1>Client Information</h1>
                <small></small>
            </div>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <!-- Form controls -->
                <div class="col-sm-12">
                    <div class="panel panel-bd lobidrag">
                        <div class="panel-body">
                            <div></div>
                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>SA Id</label>
                                    <input type="text" class="form-control" ng-model="SAId">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>First Name</label>
                                    <input type="text" class="form-control" ng-model="FirstName">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Last Name</label>
                                    <input type="text" class="form-control" ng-model="LastName">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Mobile No</label>
                                    <input type="text" class="form-control" ng-model="MobileNo" id="test">
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>Tax Ref No</label>
                                    <input type="text" class="form-control" ng-model="TaxRefNo">
                                </div>
                            </div>


                            <h5>Bank Account Details</h5>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>Bank Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Bank Name" required ng-model="BankName">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Branch Number</label>
                                    <input type="text" class="form-control" placeholder="Enter Branch Number" ng-model="BranchNumber" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Account Number</label>
                                    <input type="text" class="form-control" placeholder="Enter Account Number" ng-model="AccountNumber" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Account Type</label>
                                    <input type="text" class="form-control" placeholder="Enter Account Type" ng-model="AccountType" required>
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>Currency</label>
                                    <input type="text" class="form-control" placeholder="Enter Currency" ng-model="Currency" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Swift</label>
                                    <input type="text" class="form-control" placeholder="Enter SWIFT" ng-model="SWIFT">
                                </div>
                            </div>

                            <h5>Residential Address </h5>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>House No</label>
                                    <input type="text" class="form-control" placeholder="Enter House No" required ng-model="HouseNo">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Building Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Building Name" ng-model="BuildingName" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Floor</label>
                                    <input type="text" class="form-control" placeholder="Enter Floor" ng-model="Floor">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Flat No</label>
                                    <input type="text" class="form-control" placeholder="Enter Flat No" ng-model="FlatNo">
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>Road Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Road Name" required ng-model="RoadName">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Road No</label>
                                    <input type="text" class="form-control" placeholder="Enter Road No" ng-model="RoadNo" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Suburb Name</label>
                                    <input type="text" class="form-control" placeholder="Enter Suburb Name" ng-model="SuburbName" required>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>City</label>
                                    <input type="text" class="form-control" placeholder="Enter City" ng-model="City" required>
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <div class="form-group col-sm-3">
                                    <label>Postal Code</label>
                                    <input type="text" class="form-control" placeholder="Enter Postal Code" required ng-model="PostalCode">
                                </div>
                                <div class="form-group col-sm-3">
                                    <label>Province</label>
                                    <input type="text" class="form-control" placeholder="Enter Province" ng-model="Province" required>
                                    @*<select class="form-control" ng-model=""></select>*@
</div>
                                <div class="form-group col-sm-3">
                                    <label>Country</label>
                                    <input type="text" class="form-control" placeholder="Enter Country" ng-model="Country" required>
                                </div>

                            </div>

                            <div class="cil-sm-12">
                                <div class="form-group col-sm-4">
                                    <label>Picture upload</label>
                                    <input type="file" name="picture" ng-model="Picture">
                                    <input type="hidden" name="old_picture">
                                </div>
                            </div>



                            <div class="col-sm-12">
                                <div class="form-group col-sm-6">
                                    <button type="submit" class="btn btn-primary btn-rounded w-md m-b-5">Save</button>
                                    <button type="button" class="btn btn-danger btn-rounded w-md m-b-5">Cancel</button>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </section>
        <!-- /.content -->
    </form>
</div>






